<p>Login</p>
<%= form_for(:user, url: sessions_path) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :password %>
<%= f.password_field :password, :id => "password" %>
<%= f.submit "Sign In" %>
<% end %>
<%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %>
